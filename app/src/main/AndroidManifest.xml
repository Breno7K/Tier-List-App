<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.TierListApp"
        tools:targetApi="31">
        <activity
            android:name=".activities.auth.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.tier_list_app.activities.auth.SignUpUserActivity" />

        <activity android:name="com.example.tier_list_app.activities.home.HomeActivity" />

        <activity android:name="com.example.tier_list_app.activities.tier_list.RegistryTierListActivity" />

        <activity android:name="com.example.tier_list_app.activities.tier_list.RegistryTierActivity" />

        <activity android:name="com.example.tier_list_app.activities.tier_list.TierListViewActivity" />

        <activity
            android:name="com.example.tier_list_app.activities.tier_list.RegistryItemActivity"
            android:label="Registry Item Activity" />



    </application>

</manifest>